
(rules PCB mos6502_v1
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 102)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.0)
    )
  )
  (rule
    (width 500.0)
    (clear 500.0)
    (clear 250.0 (type smd_to_turn_gap))
    (clear 125.0 (type smd-smd))
  )
  (padstack "Via[0-1]_1000:500_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_1000:500_um" "Via[0-1]_1000:500_um" default
  )
  (via 
    "Via[0-1]_1000:500_um-kicad_default" "Via[0-1]_1000:500_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_1000:500_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_1000:500_um-kicad_default"
  )
  (class default
    "ϕ0"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND VCC "unconnected-(gal1-I1{slash}CLK-Pad1)" "Net-(D1-A)" "CS_4" "CS_5" "CS_1" "CS_2"
    "unconnected-(gal1-I10{slash}~{OE}-Pad11)" "CS_3" "Rom_Cs" "unconnected-(gal1-IO8-Pad12)" "Sram_Cs" D0 A10 A7
    A4 A15 A6 A11 A12 D7 D2 A9
    A0 A5 A2 D5 A1 A3 A8 D1
    A14 D6 A13 D4 D3 ~{WE} ~{RES} IRQ
    "unconnected-(mos1-nc-Pad36)" "unconnected-(mos1-SYNC-Pad7)" NMI "unconnected-(mos1-~{SO}-Pad38)" "unconnected-(mos1-ϕ2-Pad39)" "unconnected-(mos1-nc-Pad5)" "unconnected-(mos1-ϕ1-Pad3)" "unconnected-(mos1-nc-Pad35)"
    "unconnected-(mos1-RDY-Pad2)" "unconnected-(rom1-VPP-Pad1)" "unconnected-(rom1-~{PGM}-Pad31)" "unconnected-(J2-ID-Pad4)" "unconnected-(J2-Shield-Pad6)" "unconnected-(J2-D+-Pad3)" "unconnected-(J2-D--Pad2)" "unconnected-(J3-Pin_16-Pad16)"
    "unconnected-(sram1-NC-Pad1)" "unconnected-(J3-Pin_13-Pad13)" "unconnected-(J1-Pin_3-Pad3)" "unconnected-(J3-Pin_14-Pad14)" "unconnected-(J3-Pin_12-Pad12)" "unconnected-(J3-Pin_15-Pad15)" "unconnected-(J3-Pin_11-Pad11)" "unconnected-(J1-Pin_4-Pad4)"
    "unconnected-(J1-Pin_5-Pad5)" "unconnected-(Y1-Vcontrol-Pad1)"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)